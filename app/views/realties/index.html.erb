<h1>Realties#index</h1>
<p>Find me in app/views/realties/index.html.erb</p>

<div class="container"
   <div class="row">
      <% @realties.each do |realty| %>
         <div class="col">
            <%= realty.commune_id %>
            <%= realty.number_unit %>
            <%= realty.unit_state %>
            <%= realty.street_type_id %>
            <%= realty.population_villa %>
            <%= realty.condominium_id %>
            <%= realty.property_type %>
            <%= realty.aplle %>
            <%= realty.property %>
            <%= realty.name_realty %>
            <%= realty.latitude %>
            <%= realty.longitude %>
            <%= realty.mt2_land %>
            <%= realty.mt2_built %>
         </div>
      <% end %>
   </div>
 </div>
